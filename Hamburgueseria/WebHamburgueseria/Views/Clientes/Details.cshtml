@model WebHamburgueseria.Models.Cliente

@{
    ViewData["Title"] = "Detalle Cliente";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Detalle Cliente</h1>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Documento</dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Documento)
        </dd>
        <dt class="col-sm-2">Nombre Completo</dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NombreCompleto)
        </dd>
        <dt class="col-sm-2">Correo</dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Correo)
        </dd>
        <dt class="col-sm-2">Teléfono</dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Telefono)
        </dd>
        <dt class="col-sm-2">Usuario Registro</dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.UsuarioRegistro)
        </dd>
        <dt class="col-sm-2"> Fecha Registro</dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FechaRegistro)
        </dd>
     
     
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">Editar</a> |
    <a asp-action="Index">Volver a la Lista</a>
</div>
